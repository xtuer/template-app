QWidget {
    font-family: "云书法手书罗西硬笔行楷";
}

.CentralWidget, .CentralWidget2 {
    background: #80848f;
}

.WidgetsShower, .SettingWidget {
    background: white;
}

.WidgetsShower #previewButton {
    border-image: url(img/common/preview.png) 0;
}

.WidgetsShower #previewButton:hover {
    border-image: url(img/common/preview-hover.png) 0;
}

.WidgetsShower #previewButton:pressed {
    border-image: url(img/common/preview-pressed.png) 0;
}
